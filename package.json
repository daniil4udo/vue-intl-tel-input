{
    "name": "vue-intl-tel-input",
    "author": "Daniil Chumachenko",
    "version": "0.1.0",
    "license": "MIT",
    "description": "International telephone input on top of Buefy",
    "main": "dist/vue-intl-tel-input.cjs.js",
    "unpkg": "dist/vue-intl-tel-input.global.js",
    "jsdelivr": "dist/vue-intl-tel-input.global.js",
    "module": "dist/vue-intl-tel-input.esm-bundler.js",
    "types": "dist/vue-intl-tel-input.d.ts",
    "sideEffects": false,
    "scripts": {
        "dev": "vue-cli-service serve --port=8080",
        "dev:e2e": "cross-env VUE_APP_TEST=e2e vue-cli-service test:e2e --mode=development",
        "build": "run-s build:lib build:scss",
        "build:cli": "vue-cli-service build --target=lib --name=vue-intl-tel-input",
        "build:lib": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
        "build:lib:watch": "yarn build:lib --watch",
        "build:scss": "sass src/assets/scss/styles.scss | node ./build/banner.js > dist/vue-intl-tel-input.css && cleancss -o dist/vue-intl-tel-input.min.css dist/vue-intl-tel-input.css",
        "build:scss:watch": "sass src/assets/scss/styles.scss --watch",
        "build:ci": "yarn build --report",
        "lint": "run-s lint:all:*",
        "lint:all:eslint": "vue-cli-service lint",
        "lint:all:markdownlint": "yarn lint:markdownlint \"{src,public}/**/*.md\" \"*.md\"",
        "lint:all:stylelint": "yarn lint:stylelint \"src/**/*.{vue,scss,css}\"",
        "lint:eslint": "eslint --fix",
        "lint:markdownlint": "markdownlint --fix",
        "lint:prettier": "prettier --write --loglevel warn",
        "lint:stylelint": "stylelint --fix",
        "lint:putout": "putout --fix",
        "test": "run-s test:unit test:e2e",
        "test:unit": "cross-env VUE_APP_TEST=unit vue-cli-service test:unit",
        "test:e2e": "cross-env VUE_APP_TEST=e2e vue-cli-service test:e2e --headless",
        "test:ci": "run-s test:unit:ci test:e2e",
        "test:unit:ci": "yarn test:unit --coverage --ci",
        "test:unit:file": "yarn test:unit --bail --findRelatedTests",
        "test:unit:watch": "yarn test:unit --watch --notify --notifyMode change",
        "commit": "git-cz",
        "generate:aliases": "find ./templates -type f -name '*.aliases.json' -exec rm {} + && node aliases.config.js",
        "generate:inspect": "vue-cli-service inspect > output.js && eslint --print-config ./.eslintrc.js > .eslintrc.json"
    },
    "dependencies": {
        "awesome-phonenumber": "^2.42.0",
        "buefy": "^0.9.4",
        "core-js": "^3.6.5",
        "vue": "^2.6.11",
        "vue-class-component": "^7.2.6",
        "vue-property-decorator": "^9.1.2"
    },
    "devDependencies": {
        "@rollup/plugin-alias": "^3.1.1",
        "@rollup/plugin-commonjs": "^17.0.0",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^11.0.1",
        "@rollup/plugin-replace": "^2.3.4",
        "@types/jest": "^24.0.19",
        "@typescript-eslint/eslint-plugin": "^2.33.0",
        "@typescript-eslint/parser": "^2.33.0",
        "@vue/cli-plugin-babel": "~4.5.0",
        "@vue/cli-plugin-e2e-nightwatch": "~4.5.0",
        "@vue/cli-plugin-eslint": "~4.5.0",
        "@vue/cli-plugin-typescript": "~4.5.0",
        "@vue/cli-plugin-unit-jest": "~4.5.0",
        "@vue/cli-service": "~4.5.0",
        "@vue/compiler-sfc": "^3.0.4",
        "@vue/eslint-config-airbnb": "^5.0.2",
        "@vue/eslint-config-standard": "^5.1.2",
        "@vue/eslint-config-typescript": "^5.0.2",
        "@vue/test-utils": "^1.0.3",
        "chromedriver": "87",
        "clean-css-cli": "^4.3.0",
        "cross-env": "^7.0.3",
        "eslint": "^6.7.2",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jest": "^24.1.3",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-putout": "^6.10.0",
        "eslint-plugin-standard": "^5.0.0",
        "eslint-plugin-vue": "^7.2.0",
        "geckodriver": "^1.20.0",
        "husky": "^4.3.5",
        "lint-staged": "^10.5.3",
        "markdownlint-cli": "^0.25.0",
        "node-sass": "^5.0.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.2.1",
        "rollup": "^2.35.1",
        "rollup-plugin-babel": "^4.4.0",
        "rollup-plugin-postcss": "^4.0.0",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.29.0",
        "rollup-plugin-vue": "^5.1.6",
        "sass": "^1.30.0",
        "sass-loader": "^8.0.2",
        "stylelint": "^13.8.0",
        "stylelint-config-css-modules": "^2.2.0",
        "stylelint-config-prettier": "^8.0.2",
        "stylelint-config-recess-order": "^2.3.0",
        "stylelint-config-standard": "^20.0.0",
        "stylelint-no-unsupported-browser-features": "^4.1.4",
        "stylelint-order": "^4.1.0",
        "stylelint-scss": "^3.18.0",
        "stylelint-selector-bem-pattern": "^2.1.0",
        "stylelint-selector-no-empty": "^1.0.7",
        "stylelint-webpack-plugin": "^2.1.1",
        "typescript": "~3.9.3",
        "vue-svg-inline-loader": "^2.0.0",
        "vue-template-compiler": "^2.6.11"
    },
    "files": [
        "dist/*.js",
        "dist/vue-intl-tel-input.d.ts",
        "LICENSE",
        "README.md"
    ]
}
