{
    "name": "vue-intl-tel-input",
    "author": {
        "name": "Daniil Chumachenko",
        "email": "daniil4udo@gmail.com",
        "url": "https://github.com/dino4udo/"
    },
    "contributors": [
        {
            "name": "Democrance",
            "url": "https://democrance.com"
        }
    ],
    "version": "0.1.0",
    "license": "MIT",
    "description": "International telephone input on top of Buefy",
    "main": "dist/vue-intl-tel-input.esm.js",
    "module": "dist/vue-intl-tel-input.esm.js",
    "unpkg": "dist/vue-intl-tel-input.umd.js",
    "cjs": "dist/vue-intl-tel-input.min.js",
    "types": "dist/vue-intl-tel-input.d.ts",
    "sideEffects": false,
    "scripts": {
        "dev": "vue-cli-service serve --port=8080",
        "dev:e2e": "cross-env VUE_APP_TEST=e2e vue-cli-service test:e2e --mode=development",
        "build": "run-s build:lib build:scss",
        "build:cli": "vue-cli-service build --target=lib --name=vue-intl-tel-input",
        "build:lib": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
        "build:lib:watch": "yarn build:lib --watch",
        "build:scss": "run-s build:scss:viti build:scss:core build:scss:vendor",
        "build:scss:viti": "sass src/assets/scss/styles.scss | node ./build/banner.js > dist/css/vue-intl-tel-input.css && postcss --use autoprefixer --map false --output dist/css/vue-intl-tel-input.css dist/css/vue-intl-tel-input.css && cleancss -O2 -o dist/css/vue-intl-tel-input.min.css dist/css/vue-intl-tel-input.css",
        "build:scss:core": "sass src/assets/scss/viti.scss | node ./build/banner.js > dist/css/vue-intl-tel-input.core.css && postcss --use autoprefixer --map false --output dist/css/vue-intl-tel-input.core.css dist/css/vue-intl-tel-input.core.css && cleancss -O2 -o dist/css/vue-intl-tel-input.core.min.css dist/css/vue-intl-tel-input.core.css",
        "build:scss:vendor": "sass src/assets/scss/vendor.scss > dist/css/vue-intl-tel-input.vendor.css && postcss --use autoprefixer --map false --output dist/css/vue-intl-tel-input.vendor.css dist/css/vue-intl-tel-input.vendor.css && cleancss -O2 -o dist/css/vue-intl-tel-input.vendor.min.css dist/css/vue-intl-tel-input.vendor.css",
        "build:scss:watch": "sass src/assets/scss/styles.scss --watch",
        "build:ci": "yarn build --report",
        "lint": "run-s lint:all:*",
        "lint:all:eslint": "vue-cli-service lint",
        "lint:all:markdownlint": "yarn lint:markdownlint \"{src,public}/**/*.md\" \"*.md\"",
        "lint:all:stylelint": "yarn lint:stylelint \"src/**/*.{vue,scss,css}\"",
        "lint:eslint": "eslint --fix",
        "lint:markdownlint": "markdownlint --fix",
        "lint:prettier": "prettier --write --loglevel warn",
        "lint:stylelint": "stylelint --fix",
        "test": "run-s test:unit test:e2e",
        "test:unit": "cross-env VUE_APP_TEST=unit vue-cli-service test:unit",
        "test:e2e": "cross-env VUE_APP_TEST=e2e vue-cli-service test:e2e --headless",
        "test:ci": "run-s test:unit:ci test:e2e",
        "test:unit:ci": "yarn test:unit --coverage --ci",
        "test:unit:file": "yarn test:unit --bail --findRelatedTests",
        "test:unit:watch": "yarn test:unit --watch --notify --notifyMode change",
        "commit": "git-cz",
        "generate:inspect": "vue-cli-service inspect > output.js && eslint --print-config ./.eslintrc.js > .eslintrc.json"
    },
    "dependencies": {
        "awesome-phonenumber": "^2.43.0",
        "buefy": "^0.9.4",
        "lodash": "^4.17.19",
        "vue": "^2.6.12",
        "vue-class-component": "^7.2.6",
        "vue-property-decorator": "^9.1.2"
    },
    "devDependencies": {
        "@rollup/plugin-alias": "^3.1.1",
        "@rollup/plugin-commonjs": "^17.0.0",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^11.0.1",
        "@rollup/plugin-replace": "^2.3.4",
        "@types/jest": "^24.0.19",
        "@typescript-eslint/eslint-plugin": "^2.33.0",
        "@typescript-eslint/parser": "^2.33.0",
        "@vue/cli-plugin-babel": "~4.5.0",
        "@vue/cli-plugin-e2e-nightwatch": "~4.5.0",
        "@vue/cli-plugin-eslint": "~4.5.0",
        "@vue/cli-plugin-typescript": "~4.5.0",
        "@vue/cli-plugin-unit-jest": "~4.5.0",
        "@vue/cli-service": "~4.5.0",
        "@vue/compiler-sfc": "^3.0.4",
        "@vue/eslint-config-airbnb": "^5.3.0",
        "@vue/eslint-config-standard": "^5.1.2",
        "@vue/eslint-config-typescript": "^5.0.2",
        "@vue/test-utils": "^1.0.3",
        "autoprefixer": "^9.8.0",
        "chromedriver": "87",
        "clean-css-cli": "^4.3.0",
        "core-js": "^3.8.1",
        "cross-env": "^7.0.3",
        "eslint": "^6.7.2",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jest": "^24.1.3",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-putout": "^6.11.1",
        "eslint-plugin-standard": "^5.0.0",
        "eslint-plugin-vue": "^7.4.0",
        "geckodriver": "^1.20.0",
        "husky": "^4.3.5",
        "lint-staged": "^10.5.3",
        "markdownlint-cli": "^0.25.0",
        "npm-run-all": "^4.1.5",
        "postcss-cli": "^7.1.2",
        "postcss-import": "^12.0.0",
        "postcss-preset-env": "^6.7.0",
        "postcss-url": "^8.0.0",
        "prettier": "^2.2.1",
        "rollup": "^2.35.1",
        "rollup-plugin-copy": "^3.3.0",
        "rollup-plugin-postcss": "^4.0.0",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.29.0",
        "rollup-plugin-vue": "^5.1.6",
        "rollup-plugin-vue-inline-svg": "^1.1.1",
        "sass": "^1.30.0",
        "sass-loader": "^8.0.2",
        "stylelint": "^13.8.0",
        "stylelint-config-css-modules": "^2.2.0",
        "stylelint-config-prettier": "^8.0.2",
        "stylelint-config-recess-order": "^2.3.0",
        "stylelint-config-standard": "^20.0.0",
        "stylelint-no-unsupported-browser-features": "^4.1.4",
        "stylelint-order": "^4.1.0",
        "stylelint-scss": "^3.18.0",
        "stylelint-selector-bem-pattern": "^2.1.0",
        "stylelint-selector-no-empty": "^1.0.7",
        "stylelint-webpack-plugin": "^2.1.1",
        "typescript": "~3.9.3",
        "vue-svg-inline-loader": "^2.0.0",
        "vue-template-compiler": "^2.6.12"
    },
    "peerDependencies": {
        "awesome-phonenumber": "^2.43.0",
        "buefy": "^0.9.4",
        "lodash": "^4.17.19",
        "vue": "^2.6.12"
    },
    "files": [
        "dist/*.js",
        "dist/css/*.css",
        "dist/vue-intl-tel-input.d.ts",
        "LICENSE",
        "README.md"
    ],
    "resolutions": {
        "autoprefixer": "^9.8.0",
        "cssnano": "^4.1.0",
        "lodash": "^4.17.19",
        "postcss": "^7.0.0",
        "yargs-parser": "^18.1.2"
    }
}
